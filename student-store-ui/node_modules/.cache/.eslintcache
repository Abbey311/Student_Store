});\n                if (res?.data?.product) {\n                  setProducts(res.data.product)\n                }\n              } catch (err) {\n                console.log({err})\n                setError(\"Product Not Found\")\n              }finally{\n\n              setIsLoading(false)  \n              }\n        }\n\n        fetchProductbyId()\n    }, [id])\n    \n\n    const renderProductContent = () => {\n        if(isLoading) {\n            return <h1>Loading...</h1>\n        }\n        if(error){\n            return (\n                <>\n                <h1>Error</h1>\n                <p className=\"error\">{String(error)}</p>\n                </>\n            )\n        }\n            return (\n                <>\n        <div>\n            <div className=\"image\"><img src={products.image}></img></div>\n            <div className=\"content\">\n                <h1>{products.name}</h1>\n            </div>\n\n            <div className=\"description\">\n                <>{products.description}</>\n            </div>\n\n            <div className=\"price\">\n            <>{products.price}</>\n            </div>\n        </div>\n                </>\n            )\n        }\n\n  return (\n    <div className=\"ProductDescription\" >\n        {/* <Hero bgImage={products.image}/> */}\n            {renderProductContent()}\n    </div>\n  )\n}",{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":52,"column":36,"nodeType":"45","endLine":52,"endColumn":62},"no-native-reassign",["46"],"no-negated-in-lhs",["47"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]